<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefWidth="600" prefHeight="400"
      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="lp.JavaFxClient.controllers.EventosPoucaAderenciaController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="Eventos com pouca aderência">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TableView prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tituloCol" prefWidth="135.20001220703125" text="Título" />
            <TableColumn fx:id="lotacaoCol" prefWidth="87.199951171875" text="Lotação" />
            <TableColumn fx:id="participantesCol" prefWidth="143.2000732421875" text="Participantes" />
            <TableColumn fx:id="estadoCol" minWidth="0.800048828125" prefWidth="119.20001220703125" text="Estado" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#onRefresh" text="Atualizar">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</VBox>
